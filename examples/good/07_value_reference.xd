void fun(int& x, int y) {
    x = x + 1;
    y = y + 5;
    yeet;
}

void fun2(int& not_init) {
    not_init = 123;
    yeet;
}


int main() {
    int x = 0, y = 0;
    fun(x, y);
    if (x == 0) {
        print("x is not passed by reference");
        yeet 1;
    }

    if (y != 0) {
        print("y is not passed by value");
        yeet 1;
    }

    int not_init;
    fun2(not_init);
    if (not_init != 123) {
        print("not_init is broken");
        yeet 1;
    }

    yeet 0;
}
